public class Solution {
    public int solve(int[] A) {
        int n= A.length;
        Arrays.sort(A);
        for (int i=0;i<A.length;i++){
            if(A[i]==(n-i-1) ){
                if(A[i]==0 || A[i]!=A[i+1]){
                return 1;
                }
            }
        }
        return -1;
        
    }
}
